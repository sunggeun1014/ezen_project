<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Category">
    <select id="getCategory" resultType="category">
        SELECT
        category_num,
        category_country_type,
        category_name
        FROM
        book_categorys
    </select>
</mapper>